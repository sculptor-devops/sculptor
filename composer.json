{
    "name": "sculptor-devops/sculptor",
    "type": "project",
    "description": "Sculptor Devops Agent",
    "version": "0.8.9",
    "keywords": [
        "framework",
        "laravel",
        "laravel zero",
        "console",
        "cli",
        "devops"
    ],
    "homepage": "https://github.com/sculptor-devops",
    "support": {
        "issues": "https://github.com/sculptor-devops/sculptor/issues",
        "source": "https://github.com/sculptor-devops/sculptor"
    },
    "authors": [
        {
            "name": "Alessandro Cappellozza",
            "email": "alessandro.cappellozza@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "ext-posix": "*",
        "enlightn/security-checker": "^1.9",
        "fruitcake/laravel-cors": "^2.0",
        "graham-campbell/throttle": "^8.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.54",
        "laravel/passport": "^10.1",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "lorisleiva/cron-translator": "^0.2.1",
        "prettus/l5-repository": "^2.7",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "sculptor-devops/foundation": "^0.3.1",
        "spatie/db-dumper": "^3.0",
        "symfony/yaml": "^5.3",
	"league/flysystem-aws-s3-v3": "^1.0",
        "league/flysystem-ziparchive": "^1.0",
        "spatie/flysystem-dropbox": "^1.2"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Sculptor\\Agent\\": "src"
        },
	"files": [
            "src/Constants.php",
            "src/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
